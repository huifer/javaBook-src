<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.github.huifer.full.shiro.dao.ShiroResourceDao">

  <select id="findByNameAndAppIdAndType"
    resultType="com.github.huifer.full.shiro.entity.ShiroResource">
    select  * from shiro_resource where app_id = #{appId} and name = #{name} and type = #{type}
  </select>
</mapper>