<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.github.huifer.full.shiro.dao.ShiroRoleDao">

  <select id="findByNameAndCode" resultType="com.github.huifer.full.shiro.entity.ShiroRole">
    select * from  shiro_role where  1=1 and name = #{name} and code = #{code}
  </select>
</mapper>